												function(e) {
													//Get window size:
													var winWidth = $(document)
															.width();
													var winHeight = $(document)
															.height();
													//Get pointer position:
													var posX = e.pageX;
													var posY = e.pageY;
													//Get contextmenu size:
													var menuWidth = $(
															".contextmenu")
															.width();
													var menuHeight = $(
															".contextmenu")
															.height();
													//Security margin:
													var secMargin = 10;
													//Prevent page overflow:
													if (posX + menuWidth
															+ secMargin >= winWidth
															&& posY
																	+ menuHeight
																	+ secMargin >= winHeight) {
														//Case 1: right-bottom overflow:
														posLeft = posX
																- menuWidth
																- secMargin
																+ "px";
														posTop = posY
																- menuHeight
																- secMargin
																+ "px";
													} else if (posX + menuWidth
